#!/usr/bin/env bash
# This script is displaying "Best School" 10 times

a="1"
while [ $a -le 20 ]
do
    echo "$a"
    case $a in
	4)
	    echo "$a bad luck from China"
	    ;;
	9)
	    echo "$a bad luck from Japan"
	    ;;
	17)
            echo "$a bad luck from Italy"
	    ;;
	esac
    a=$((a+1)) 
done
